 <div id="resultbutton">
/% CODEWORD INPUT %/
<div class="code-input">
<<textbox "$code" "">>
<<button "Send">>
</div>
 /% PASSWORD CODE %/
<<set $code to $code.toUpperCase()>>
/% CODEWORD CORRECT %/
<<if $code isnot "">> 
  <<if $code is "CLIMATE" or $code is "CLIMATE ">>
    <<audio "correct" play>>
    <<replace "#helperText">><div class="confirmation" id="confirmation">Correct!</div><</replace>>
     <<timed 0.5s>>
      <<replace "#scrabbleWrapper">>
         <div class="scrabble-container">
         <div id="pos1"> <div class="tile" data-letter="C"></div></div>
      <div id="pos2"> <div class="tile" data-letter="L"></div></div>
      <div id="pos3"><div class="tile" data-letter="I"></div></div>
      <div id="pos4"><div class="tile" data-letter="M"></div></div>
        <div id="pos5"><div class="tile" data-letter="A"></div></div>
        <div id="pos6"><div class="tile" data-letter="T"></div></div>
       <div id="pos7"><div class="tile" data-letter="E"></div> 
       </div>
       </div>      
      <</replace>>
    <</timed>>

    <<timed 3s>><<goto $nextpass>><</timed>>
   
<</if>>
<</if>>
/% CODEWORD INCORRECT %/
<<if $code isnot "">>
  <<if $code is not "CLIMATE" and $code is not "CLIMATE ">>
  <<replace "#helperText">><div class="confirmation" id="confirmation">That's not right...</div><</replace>>
  <<timed 1.5s>><<replace "#helperText">><span>Enter the correct word:</span><</replace>><</timed>>
  <</if>>
  <</if>>
<</button>>
</div>

</div>

